  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build": "bin/build",
    "graphql:codegen:admin": "cd apps/admin-backend && ./bin/graphql-codegen",
    "dev:initialize": "bin/dev-initialize",
    "dev:admin": "bin/dev-admin",
    "start:admin": "bin/start-admin",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv/config node_modules/.bin/jest --config jest.config.js",
    "db:migrate:generate:test": "DOTENV_CONFIG_PATH=.env.test ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/typeorm migration:generate -n",
    "db:migrate:generate:local": "DOTENV_CONFIG_PATH=.env.localhost ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/typeorm migration:generate -n",
    "db:migrate:run:test": "DOTENV_CONFIG_PATH=.env.test ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/typeorm migration:run",
    "db:migrate:run:local": "DOTENV_CONFIG_PATH=.env.localhost ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/typeorm migration:run",
    "db:seeds:local": "DOTENV_CONFIG_PATH=.env.localhost ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:migrate:revert:test": "DOTENV_CONFIG_PATH=.env.test ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/typeorm migration:revert",
    "db:migrate:revert:local": "DOTENV_CONFIG_PATH=.env.localhost ts-node -r dotenv/config -r tsconfig-paths/register node_modules/.bin/typeorm migration:revert"
  },
