    "@nuxtjs/pwa": "^3.3.5",
    "aws-amplify": "^4.3.4",
    "core-js": "^3.15.1",
    "graphql": "^15.5.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "nuxt": "^2.15.7",
    "nuxt-typed-vuex": "^0.2.0",
    "ts-custom-error": "^3.2.0",
    "typed-vuex": "^0.2.0",
    "vee-validate": "^3.4.14",
