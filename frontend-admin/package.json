{
  "name": "frontend-liff",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3002 DOTENV_CONFIG_PATH=.env.localhost node -r dotenv/config ./node_modules/.bin/nuxt",
    "build:dev": "DOTENV_CONFIG_PATH=.env.dev node -r dotenv/config ./node_modules/.bin/nuxt build",
    "build:prd": "DOTENV_CONFIG_PATH=.env.prd node -r dotenv/config ./node_modules/.bin/nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "graphql:codegen": "./bin/graphql-codegen",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
